<% @users.each do |user| %>
<h3><%= user.username %></h3> 
<table class="table">
<head>
<tr>
<th>id</th>
<th>message</th>
<th>views</th>
</tr>
</head>
<tbody>
<% @users.each do |user| %>
    <% user.twitter_messages.each do |twitter_message| %>
        <tr>
        <td><%= twitter_message.timeline_id %></td>
        <td><%= twitter_message.data['text'] %></td>
        <td><%= twitter_message.data['retweet_count'] %></td>
        </tr>
    <% end %>
<% end %>
</tbody>
</table>
<% end %>
